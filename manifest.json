{
    "manifest_version": 2,
    "name": "Link Tracker",
    "version": "0.2",
    "permissions": [
        "tabs",
        "webRequest",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": "icon-48.png"
    }
}
