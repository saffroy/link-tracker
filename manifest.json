{
    "manifest_version": 2,
    "name": "URL Filter Extension",
    "version": "1.0",
    "permissions": [
        "tabs",
        "webRequest",
        "<all_urls>"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}
