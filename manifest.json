{
    "manifest_version": 3,
    "name": "Link Tracker",
    "version": "0.3",
    "permissions": [
        "tabs",
        "webRequest",
        "storage"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon-48.png"
    }
}
